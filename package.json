{
  "name": "ng-tower",
  "version": "0.0.1",
  "author": "armorik83",
  "bugs": {
    "url": "https://github.com/armorik83/ng-tower/issues"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/core": "^2.0.0-rc.4",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4",
    "core-js": "^2.4.0",
    "rxjs": "^5.0.0-beta.10",
    "walts": "^0.8.0-beta.5",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^2.5.3",
    "node-notifier": "^4.6.0",
    "power-assert": "^1.4.1",
    "superstatic": "^4.0.2",
    "tslint": "^3.13.0",
    "typescript": "^2.0.0",
    "typings": "^1.3.1",
    "webpack": "^1.13.1"
  },
  "homepage": "https://github.com/armorik83/ng-tower#readme",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/armorik83/ng-tower.git"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && npm run tsc && npm run webpack && npm run done",
    "clean": "rm -rf ./.tmp",
    "done": "notify -t \"Done\" -m \"Build has done.\" -o http://127.0.0.1:8080",
    "lint": "./tslint.sh",
    "postinstall": "typings install",
    "start": "superstatic --port 8080 --host 127.0.0.1",
    "test": "npm run clean && npm run tsc && mocha --require intelli-espower-loader --reporter dot ./.tmp/test/**/*.spec.js",
    "tsc": "tsc -p .",
    "webpack": "webpack ./.tmp/app/index.js --output-filename ./built/index.js"
  }
}
